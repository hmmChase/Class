{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "start": "node ./build/app.js",
    "seed": "node ./src/prisma/seed.js",
    "clean": "rimraf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s --copy-files",
    "build": "npm run clean && npm run build-babel",
    "migrate:dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "migrate:prod": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "postinstall": "npm run generate && npm run migrate:prod"
  },
  "dependencies": {
    "@prisma/client": "^2.22.1",
    "@sendgrid/mail": "^7.4.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "discord-oauth2": "^2.6.0",
    "dotenv": "^9.0.2",
    "ejs": "^3.1.6",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.6.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "juice": "^7.0.0",
    "morgan": "~1.10.0",
    "nodemailer": "^6.6.0",
    "rimraf": "^3.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "prisma": "^2.22.1"
  }
}

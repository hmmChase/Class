{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "seed": "node ./src/prisma/seed",
    "clean": "rimraf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "vercel-build": "npm run clean && npm run build-babel && prisma generate",
    "start-build": "npm run build && node ./build/index.js",
    "start": "node ./build/index.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@prisma/client": "^2.22.0",
    "@sendgrid/mail": "^7.4.2",
    "argon2": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "discord-oauth2": "^2.6.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.6.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "juice": "^7.0.0",
    "morgan": "~1.10.0",
    "nodemailer": "^6.6.0",
    "prisma": "^2.21.2",
    "rimraf": "^3.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7"
  }
}

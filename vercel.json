{
  "alias": "challenge-board",
  "version": 2,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "backend/bin/www",
      "use": "@now/node-server"
    },
    {
      "src": "frontend/src/index.js",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/bin/www"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1",
      "continue": true
    }
  ]
}

{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon ./src/app.js",
    "start": "node ./src/app.js",
    "seed": "node ./src/prisma/seed.js",
    "migrate:dev": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "migrate:prod": "prisma migrate deploy",
    "generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "postinstall": " npm run generate && npm run migrate:prod"
  },
  "dependencies": {
    "@prisma/client": "^2.22.0",
    "@sendgrid/mail": "^7.4.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.1",
    "discord-oauth2": "^2.6.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.6.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "juice": "^7.0.0",
    "morgan": "~1.10.0",
    "nodemailer": "^6.6.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "prisma": "^2.22.0"
  }
}
